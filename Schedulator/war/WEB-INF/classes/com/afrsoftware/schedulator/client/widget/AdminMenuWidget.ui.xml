<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.afrsoftware.schedulator.client.widget">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<c:HeaderWidget/>
		<g:HTMLPanel styleName="container">
			<g:HTMLPanel styleName="row-fluid">
				<g:HTMLPanel styleName="span4 adminButton well">
					<i class="icon-search icon-4x"></i>
					<h3>Index des clients</h3>
					<p class="adminDescription">Rechercher les clients, mettre à jour leurs informations
						et ajouter des forfaits.</p>
					<g:Button ui:field="search" styleName="btn btn-primary">Index des clients
						&raquo;</g:Button>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="span4 adminButton well">
					<i class="icon-plus-sign icon-4x"></i>
					<h3>Inscrire un client</h3>
					<p class="adminDescription">Ajouter un nouveau client et lui assigner un forfait.</p>
					<g:Button ui:field="add" styleName="btn btn-primary">Inscrire un client
						&raquo;</g:Button>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="span4 adminButton well">
					<i class="icon-calendar icon-4x"></i>
					<h3>Calendrier</h3>
					<p class="adminDescription">Voir et ajouter les évenements et voir les clients
						inscrits.</p>
					<g:Button ui:field="calendar" styleName="btn btn-primary">Calendrier &raquo;</g:Button>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 